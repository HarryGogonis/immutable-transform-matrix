{
  "name": "immutable-transform-matrix",
  "version": "0.0.0-development",
  "license": "MIT",
  "description": "A matrix library using ImmutableJS data structutes",
  "keywords": [
    "matrix",
    "svg",
    "immutable",
    "ctm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HarryGogonis/immutable-transform-matrix"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
    "lint": "standard src tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:dev": "npm run test -- --watch",
    "prepublish": "npm run clean & npm run build",
    "docs": "jsdoc2md src/index.js > docs/README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "jest": "^20.0.3",
    "jsdoc-to-markdown": "^3.0.0",
    "rimraf": "^2.5.2",
    "standard": "^10.0.2",
    "semantic-release": "^6.3.6"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "extendable-immutable": "^1.3.2",
    "immutable": "^3.8.1"
  }
}
